	TeleportService:TeleportToPlaceInstance(game.PlaceId, game.JobId, Player)
end)

--Search Bar

GUI.Frame.Search:GetPropertyChangedSignal("Text"):Connect(function()
	local Text = GUI.Frame.Search.Text
	for _,Bar in pairs(UIFrame:GetChildren()) do
		if not Bar:IsA("Frame") then continue end
		if #Text < 1 or Text == "" then
			Bar.Visible = true
			continue
		end
		if string.find(string.lower(Bar.Name), string.lower(Text)) then
			Bar.Visible = true
		else
			Bar.Visible = false
		end
	end
end)

--Mouse

Mouse.Button1Up:Connect(function()
	if Mouse.Target then
		local Target = Mouse.Target
		if not Target:IsA("BasePart") then
			return
		end
		if Platform then
			if Platform == Target then 
				return
			end
		end
		if Phase.Value then
			if Target.Name == "Baseplate" then
				return
			end
			if Target.CanCollide == true then
				Target.CanCollide = false
				local PhaseObject = HighlightObject:Clone()
				PhaseObject.Name = "PhaseObj"
				PhaseObject.Parent = Target
				PhaseObject.Adornee = Target
				table.insert(PhaseObjects, Target)
			elseif Target:FindFirstChild("PhaseObj") then
				Target.CanCollide = true
				local PhaseObject = Target:FindFirstChild("PhaseObj")
				if PhaseObject then
					PhaseObject:Destroy()
				end
				table.remove(PhaseObjects, table.find(PhaseObjects, Target))
			end
		elseif Teleport.Value then
			local Location = Mouse.Hit.Position
			local Root = Player.Character:FindFirstChild("HumanoidRootPart")
			if Root then
				Player.Character:MoveTo(Location)
			end
		end
	end
end)

Player.Character:WaitForChild("Humanoid").Died:Connect(function()
	if Staircases then
		Staircases:ClearAllChildren()
	end
	if Platform then
		Platform:Destroy()
	end
	Teleport.Value = false
	Phase.Value = false
	for _,Object in pairs(PhaseObjects) do
		Object.CanCollide = true
		local PhaseObject = Object:FindFirstChild("PhaseObj")
		if PhaseObject then
			PhaseObject:Destroy()
		end
	end
	PhaseObjects = {}
	Markers = nil
end)

Player.CharacterAdded:Connect(function(Character)
	Character:WaitForChild("Humanoid").Died:Connect(function()
		if Staircases then
			Staircases:ClearAllChildren()
		end
		if Platform then
			Platform:Destroy()
		end
		Teleport.Value = false
		Phase.Value = false
		for _,Object in pairs(PhaseObjects) do
			Object.CanCollide = true
			local PhaseObject = Object:FindFirstChild("PhaseObj")
			if PhaseObject then
				PhaseObject:Destroy()
			end
		end
		PhaseObjects = {}
		Markers = nil
	end)
end)
