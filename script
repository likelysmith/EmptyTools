	local Amt = UIFrame.Emote.ID
	if not tonumber(Amt.Text) then
		return
	end
	PlayEmote(tonumber(Amt.Text))
end)
UIFrame.Emote.Stop.MouseButton1Click:Connect(function()
	Player.Character.Animate.Disabled = false
	local Anim = Player.Character.Humanoid:GetPlayingAnimationTracks()
	for i,v in pairs(Anim) do
		v:Stop()
	end
end)

UIFrame.Fly.Run.MouseButton1Click:Connect(function()
	if selected then
		ToggleFlight(false)
		wait(0.05)
	end
	ToggleFlight(true)
end)

UIFrame.Fly.Stop.MouseButton1Click:Connect(function()
	ToggleFlight(false)
end)
UIFrame.Fly.Speed.Text = FlySpeed
UIFrame.Fly.Speed:GetPropertyChangedSignal("Text"):Connect(function()
	local Text = UIFrame.Fly.Speed
	if tonumber(Text.Text) then
		local Speed = tonumber(Text.Text)
		if Speed > 100 then
			Speed = 100
			Text.Text = Speed
		end
		FlySpeed = Speed
	end
end)

UIFrame.Markers.Run.MouseButton1Click:Connect(function()
	SetupTeleportMarkers()
